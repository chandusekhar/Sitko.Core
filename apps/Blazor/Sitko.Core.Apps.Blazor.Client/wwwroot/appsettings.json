{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MudLayout": {
    "Theme": "Dark"
  },
  "Application": {
    "Name": "Sitko.Core.Apps.Blazor",
    "Version": "11.0.0"
  },
  "Storage": {
    "Remote": {
      "RemoteStorageOptions": {
        "PublicUri": "https://localhost:7163/Static/",
        "Name": "Remote",
        "RemoteUrl": "https://localhost:7163/Upload/"
      }
    }
  },
  "Repositories": {
    "Remote": {
      "Transports": {
        "Http": {
          "RepositoryControllerApiRoute": "https://localhost:7163/api"
        }
      }
    }
  }
}
